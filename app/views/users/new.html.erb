<h2>Sign Up</h2>
<%= form_for @user do |f|%>
	<%= render "shared/error_messages", object: @user %>
	<%= f.label :email %> 
	<%= f.email_field :email %>
	<%= f.label :password %> 
	<%= f.password_field :password %>
	<%= f.label :password_confirmation %> 
	<%= f.password_field :password_confirmation %>
	<%= f.submit "Sign up!" %>
<%end%>